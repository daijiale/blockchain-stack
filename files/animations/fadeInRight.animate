/* 
  Name:			Fade In Right
  Type:			Entrance
  Description:	Fade In Effect
  Version:		2.0	
  locale:		{"zh-Hans":"右边渐入", "en-US":"Fade In Right"}
 */
@keyframes fadeInRight {
  0% {
    opacity: 0;
    transform: translate3d(100%, 0, 0);
  }

  100% {
    opacity: 1;
    transform: none;
  }
}

.fadeInRight {
  animation-name: fadeInRight;
}